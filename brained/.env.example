# Root .env.example for the brained workspace
# Copy this file to .env (or create .env in server/) and fill the values.

# MongoDB Atlas connection string (URI)
MONGO_URI=your-mongodb-atlas-connection-string

# JWT secret used to sign tokens
JWT_SECRET=your_jwt_secret_here

# Port for Express server
PORT=5000

# URL of frontend client (used for CORS)
CLIENT_URL=http://localhost:3000
